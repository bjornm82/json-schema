{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "#enum",
    "type": "object",
    "required": [
        "type",
        "name",
        "symbols"
    ],
    "properties": {
        "type": {
            "$id": "#/properties/type",
            "type": "string",
            "enum": [
                "enum"
            ]
        },
        "name": {
            "$id": "#/properties/name",
            "type": "string"
        },
        "symbols": {
            "$id": "#/properties/symbols",
            "type": "array",
            "additionalItems": false,
            "items": {
                "$id": "#/properties/symbols/items",
                "anyOf": [
                    {
                        "$id": "#/properties/symbols/items/anyOf/0",
                        "type": "string"
                    }
                ]
            }
        }
    },
    "additionalProperties": false
}